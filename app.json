{
  "expo": {
    "name": "plushie",
    "slug": "plushie",
    "version": "1.0.0",
    "scheme": "plushie",
    "orientation": "landscape",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/plushiechronicles.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "icon": "./assets/playboxstudio.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/playboxstudio.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.plushie.chronicles"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/playboxstudio.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.plushie.chronicles"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "15f1a973-f05c-4e1b-a5bb-8d3f7543f4b9"
      }
    }
  }
}